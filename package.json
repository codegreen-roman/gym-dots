        "react-redux": "^5.0.5",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1",
        "redux": "^3.6.0",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-core": "^6.24.1",
        "babel-loader": "^7.0.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^0.28.4",
        "eslint": "^3.19.0",
        "eslint-config-standard-react": "^5.0.0",
        "eslint-loader": "^1.7.1",
        "eslint-plugin-react": "^7.0.1",
        "flow-bin": "^0.47.0",
        "json-loader": "^0.5.4",
        "json-server": "^0.10.1",
        "nodemon": "^1.11.0",
        "npm-run-all": "^4.0.2",
        "style-loader": "^0.18.2",
        "webpack": "^2.6.1",
        "webpack-dev-server": "^2.4.5"
