    "author": "Roman Shestakov <neoroma@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "webpack",
        "dev": "webpack-dev-server",
        "lint-fix": "node_modules/.bin/eslint --fix js/**/*.js",
        "lint": "node_modules/.bin/eslint js/**/*.js webpack.config.js",
        "watch": "npm run build -- --watch",
        "server": "nodemon --watch server --exec babel-node -- server/index.js",
        "data:mockapi": "json-server data/data.json",
        "dev:server": "npm-run-all --parallel dev data:mockapi"
    },
    "dependencies": {
        "express": "^4.15.3",
        "prop-types": "^15.5.10",
        "ramda": "^0.24.1",
